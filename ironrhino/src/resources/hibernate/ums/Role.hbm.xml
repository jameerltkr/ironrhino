<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.ums.model"
	default-lazy="true">
	<class name="Role" table="ums_role">
		<id name="id" type="java.lang.String" length="32"
			unsaved-value="null">
			<generator class="uuid" />
		</id>
		<natural-id>
			<property name="name" not-null="true" />
		</natural-id>
		<property name="description" />
		<property name="enabled" />
		<set name="groups" table="ums_group_role" cascade="none"
			inverse="false" lazy="true">
			<key column="roleId" />
			<many-to-many class="Group" column="groupId" />
		</set>
	</class>
</hibernate-mapping>