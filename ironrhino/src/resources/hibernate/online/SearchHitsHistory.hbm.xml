<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.online.model"
	default-lazy="true">
	<class name="SearchHitsHistory" table="online_searchHitsHistory">
		<id name="id" type="java.lang.String" length="32"
			unsaved-value="null">
			<generator class="uuid" />
		</id>
		<natural-id>
			<property name="keyword" />
			<property name="month" />
			<property name="year" />
		</natural-id>
		<property name="count" />
	</class>
</hibernate-mapping>