	<bean id="userManager"
		class="org.ironrhino.ums.service.UserManagerImpl" />
	<bean id="groupManager"
		class="org.ironrhino.ums.service.GroupManagerImpl" />
	<bean id="roleManager"
		class="org.ironrhino.ums.service.RoleManagerImpl" />
</beans>
