<?xml version="1.0" encoding="UTF-8"?>
<beans default-autowire="byName"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="paymentManager"
		class="org.ironrhino.online.payment.PaymentManager">
		<property name="payments">
			<list>
				<ref bean="paypal" />
				<ref bean="alipay" />
			</list>
		</property>
	</bean>
	<bean id="paypal" class="org.ironrhino.online.payment.Paypal"
		p:business="${paypal.business}"/>
	<bean id="alipay" class="org.ironrhino.online.payment.Alipay"
		p:partner="${alipay.partner}" p:key="${alipay.key}" />
</beans>
